    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/microsoft/tabster"
    },
    "scripts": {
        "build": "npm run clean && tsdx build && npm run build-storybook",
        "build-storybook": "build-storybook",
        "clean": "rimraf dist",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "lint": "eslint src/ --fix",
